{
  "name": "@hurdlegroup/robotjs",
  "version": "0.0.0-development",
  "description": "Node.js Desktop Automation.",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "test": "run-script-os",
    "test:darwin:linux": "jasmine test/**/*.js",
    "test-keyboard": "node test/keyboard.js",
    "test:win32": "jasmine test/**/*.js",
    "install": "node-gyp-build",
    "install-debug": "node-gyp-build --debug",
    "prebuild": "prebuildify -t 16.0.0 --napi --strip",
    "prebuild-darwin-universal": "prebuildify -t 16.0.0 --napi --strip --arch x64+arm64",
    "prebuild-linux-x64": "prebuildify -t 16.0.0 --napi --strip",
    "prebuild-win32-x86": "prebuildify -t 16.0.0 --napi --strip",
    "prebuild-win32-x64": "prebuildify -t 16.0.0 --napi --strip"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hurdlegroup/robotjs"
  },
  "maintainers": [
    {
      "name": "Daniel McAssey",
      "email": "daniel.mcassey@hurdle.live"
    }
  ],
  "keywords": [
    "Automation",
    "GUI",
    "mouse",
    "keyboard",
    "screenshot",
    "image",
    "pixel",
    "desktop",
    "robotjs",
    "screen",
    "recognition",
    "autohotkey",
    "machine",
    "learning",
    "color"
  ],
  "author": "Jason Stallings",
  "license": "MIT",
  "gypfile": true,
  "binary": {
    "napi_versions": [
      3
    ]
  },
  "bugs": {
    "url": "https://github.com/hurdlegroup/robotjs/issues"
  },
  "homepage": "https://github.com/hurdlegroup/robotjs",
  "dependencies": {
    "node-addon-api": "^4.3.0",
    "node-gyp-build": "^4.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "jasmine": "^2.99.0",
    "prebuildify": "^5.0.1",
    "run-script-os": "^1.1.6",
    "tape": "^4.16.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^8.0.3"
  }
}
